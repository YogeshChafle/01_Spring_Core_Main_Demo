<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:p="http://www.springframework.org/schema/p"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean class= "springdemo.yogesh.student" name= "student1">
		<property name = "id" value = "101"/>
		<property name="name" value = "Yogesh" />
		<property name= "address" value="Hinganghat"/>
	</bean>
	
	<bean  class= "springdemo.yogesh.student" name= "student2" p:id="120" p:name="pankaj" p:address="Sirud" />

</beans>